<p id="notice"><%= notice %></p>

<h1>Articles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Browsecount</th>
      <th>Likecount</th>
      <th>Learnuser</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.content %></td>
        <td><%= article.browseCount %></td>
        <td><%= article.likeCount %></td>
        <td><%= article.learn_user_id %></td>
        <td><%= link_to 'Show', "/frankgogo/adminarticles/#{article.id}"%></td>
        <td><%= link_to 'Edit', edit_adminarticle_path(article) %></td>
        <td><%= link_to 'Destroy', article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Article', new_adminarticle_path %>
